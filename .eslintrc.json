{
  "root": true,
  "$schema": "https://json.schemastore.org/eslintrc",
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "google",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "require-jsdoc": "off",
    "new-cap": "off",
    "prettier/prettier": "error"
  }
}
